// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  @@map("users")
  id Int @id @default(autoincrement())
  userId String @unique
  email String @unique
  firstname String
  lastname String
  password String
  company String
}